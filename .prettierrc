{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "arrowParens": "always",
    "overrides": [
        {
            "files": [".*.js"],
            "options": {
                "parser": "json"
            }
        },
        {
            "files": ["*.js"],
            "options": {
                "parser": "babel"
            }
        }
    ]
}